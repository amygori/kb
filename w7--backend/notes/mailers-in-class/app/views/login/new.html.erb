<h1>Login#new</h1>
<p>Find me in app/views/login/new.html.erb</p>

<div class="login-form">
 <%= form_tag login_create_path do %>
    <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    </div>
    <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    </div>
    <div>
      <%= button_to :submit %>
    </div>

 <% end %>
</div>